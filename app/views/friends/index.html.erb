<div class="page-header">
  <h1>
    Friends of <%=@current_user.name %>
  </h1>
</div>

<div class="row">
  <div class="col-md-3">
    <table class="table table-striped table-hover">
      <tr>

        <th>Friend's Name</th>

      </tr>

      <% @current_user_friends.each do |friend| %>
      <tr>
        <td> <%= friend["name"] %></td>


      </tr>
      <% end %>
    </table>
  </div>
</div>
