<div class="page-header">
  <h1>
    Users
    <a href="/users/new" class="btn btn-lg btn-success">New User</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Uid</th>
        <th>Provider</th>
        <th>Name</th>
        <th>Access token</th>
        <th>Actions</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td><%= user.uid %></td>
        <td><%= user.provider %></td>
        <td><%= user.name %></td>
        <td><%= user.access_token %></td>
        <td>
          <a href="/users/<%= user.id %>" class="btn btn-primary">Show</a>
          <a href="/users/<%= user.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_user/<%= user.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
